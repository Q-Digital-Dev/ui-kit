{
  "name": "ui-kit",
  "version": "1.0.1",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.svg dist/",
    "build": "yarn clean && tsc && yarn copy-files",
    "watch": "yarn clean && tsc -w && yarn copy-files"
  },
  "exports": {
    ".": {
      "import": "./dist/web/index.js",
      "default": "./dist/web/index.js",
      "types": "./dist/web/index.d.ts"
    }
  },
  "react-native": "./dist/native/index.js",
  "types": "./dist/native/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Q-Digital-Dev/ui-kit.git"
  },
  "keywords": [
    "ui",
    "ui-kit",
    "react",
    "react-native",
    "design system",
    "library",
    "q-digital"
  ],
  "author": "Q-digital",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Q-Digital-Dev/ui-kit/issues"
  },
  "homepage": "https://github.com/Q-Digital-Dev/ui-kit#readme",
  "devDependencies": {
    "@tsconfig/react-native": "3.0.3",
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-input-mask": "3.0.5",
    "classnames": "2.3.2",
    "copyfiles": "2.4.1",
    "nativewind": "2.0.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-input-mask": "2.0.4",
    "react-native": "0.73.5",
    "react-native-mask-input": "1.2.3",
    "react-native-svg": "15.0.0",
    "rimraf": "5.0.5",
    "tsc": "2.0.4",
    "typescript": "5.3.3"
  }
}
